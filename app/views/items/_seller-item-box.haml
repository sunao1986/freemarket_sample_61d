- seller_items.each do |item|
  .seller-item-box
    %a.seller-item-box{href:"/items/#{item.id}"}
      .seller-item-box__photo
        = image_tag item.images[0].image_url
        .seller-item-box__info
          .seller-item-box__info__name
            = item.name
          .seller-item-box__info__num
            .seller-item-box__info__name__price
              = "Â¥#{item.price.to_s(:delimited)}"